{
    "name": "specialistaweb/newsstylist",
    "type": "typo3-cms-extension",
    "minimum-stability":"dev",
    "description": "News stylist - Extends news system, list view, with 5 layouts and several options to repositioning the objects; swiperlider included.",
    "authors": [
        {
            "name": "Alessandro Tuveri",
            "role": "Developer"
        },
        {
            "name": "Giulia Tuveri",
            "role": "Developer"
        }
    ],
    "keywords": [
        "TYPO3",
        "extension",
        "article",
        "news",
        "list",
        "slider",
        "swiper",
        "layout",
        "template",
	"styles",
	"carousel",
	"card",
	"coverflow",
	"newsletter",
	"text"
    ],
    "homepage": "https://typo3.specialistaweb.it/development/newsstylist",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "georgringer/news": ">= 14.0 > 14.4",
        "bk2k/bootstrap-package": ">= 16.0 > 16.4"
    },
    "autoload": {
        "psr-4": {
            "SpecialistaWeb\\Newsstylist\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "newsstylist"
        }
    }
}
